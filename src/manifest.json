{
  "manifest_version": 3,
  "name": "GitHub Wiki Search",
  "version": "2.0.0",
  "description": "Simple search box for GitHub wikis.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"]
    }
  ]
}
